{
  "name": "angular-svg-round-progressbar",
  "version": "7.0.0",
  "description": "Angular module that uses SVG to create a circular progressbar",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-demo": "ng build demo --configuration production",
    "build-lib": "ng build angular-svg-round-progressbar --configuration production && npm run copy-readme",
    "release-lib": "npm run lint && npm run build-lib && npm publish ./dist/angular-svg-round-progressbar",
    "release-demo": "npm run lint && ng deploy demo",
    "lint": "ng lint",
    "copy-readme": "cp README.md dist/angular-svg-round-progressbar"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/crisbeto/angular-svg-round-progressbar.git"
  },
  "keywords": [
    "angular",
    "progress",
    "circle",
    "round",
    "svg"
  ],
  "author": "crisbeto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crisbeto/angular-svg-round-progressbar/issues"
  },
  "homepage": "https://github.com/crisbeto/angular-svg-round-progressbar",
  "peerDependencies": {
    "@angular/core": "^12.0.0",
    "@angular/common": "^12.0.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1200.4",
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@angular/core": "~12.0.4",
    "@angular/forms": "~12.0.4",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "codelyzer": "^6.0.0",
    "ng-packagr": "^12.0.5",
    "rxjs": "~6.6.3",
    "tslib": "^2.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4",
    "zone.js": "~0.11.4"
  }
}
